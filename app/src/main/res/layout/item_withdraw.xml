            <TextView
                android:id="@+id/textAccNumber"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginTop="6dp"
                android:text="Account Number :"
                android:textColor="@color/white"
                android:textSize="@dimen/sp_18"
                app:layout_constraintStart_toStartOf="@+id/textAccHolderName"
                app:layout_constraintTop_toBottomOf="@+id/textAccHolderName" />

            <TextView
                android:id="@+id/atvAccNumber"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginStart="3dp"
                android:text="12345678912"
                android:textColor="@color/white"
                android:textSize="@dimen/sp_18"
                android:textStyle="bold"
                app:layout_constraintBottom_toBottomOf="@+id/textAccNumber"
                app:layout_constraintStart_toEndOf="@+id/textAccNumber" />

            <TextView
                android:id="@+id/textIFSCCode"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginTop="6dp"
                android:text="Account Number :"
                android:textColor="@color/white"
                android:textSize="@dimen/sp_18"
                app:layout_constraintStart_toStartOf="@+id/textAccNumber"
                app:layout_constraintTop_toBottomOf="@+id/textAccNumber" />

            <TextView
                android:id="@+id/atvIFSCCode"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginStart="3dp"
                android:text="SBIN00000"
                android:textColor="@color/white"
                android:textSize="@dimen/sp_18"
                android:textStyle="bold"
                app:layout_constraintBottom_toBottomOf="@+id/textIFSCCode"
                app:layout_constraintStart_toEndOf="@+id/textIFSCCode" />



            <TextView
                    android:id="@+id/btApprove"
                    android:layout_width="@dimen/dp_100"
                    android:layout_height="wrap_content"
                    android:layout_marginTop="12dp"
                    android:layout_marginBottom="6dp"
                    android:background="@drawable/button_two"
                    android:text="Approve"
                    android:textAlignment="center"
                    android:textColor="@color/white"
                    app:layout_constraintBottom_toBottomOf="parent"
                    app:layout_constraintEnd_toStartOf="@id/btDeny"
                    app:layout_constraintStart_toStartOf="parent"
                    app:layout_constraintTop_toBottomOf="@id/textIFSCCode" />

                <TextView
                    android:id="@+id/btDeny"
                    android:layout_width="@dimen/dp_100"
                    android:layout_height="wrap_content"
                    android:layout_marginTop="12dp"
                    android:layout_marginBottom="6dp"
                    android:background="@drawable/button_two"
                    android:text="Deny"
                    android:textAlignment="center"
                    android:textColor="@color/white"
                    app:layout_constraintBottom_toBottomOf="parent"
                    app:layout_constraintEnd_toEndOf="parent"
                    app:layout_constraintStart_toEndOf="@id/btApprove"
                    app:layout_constraintTop_toBottomOf="@id/textIFSCCode"
                     />

            </androidx.constraintlayout.widget.ConstraintLayout>


